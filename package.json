{
  "name": "schibsted-style",
  "version": "0.0.0",
  "description": "Meta package for ESLint settings for Schibsted",
  "private": true,
  "repository": "schibsted/eslint-config-schibsted",
  "license": "MIT",
  "scripts": {
    "install": "npm run install:schibsted && npm run install:schibsted-react",
    "install:schibsted": "cd packages/eslint-config-schibsted && yarn",
    "install:schibsted-react": "cd packages/eslint-config-schibsted-react && yarn",
    "test": "npm run test:schibsted && npm run test:schibsted-react",
    "test:schibsted": "cd packages/eslint-config-schibsted && npm test",
    "test:schibsted-react": "cd packages/eslint-config-schibsted-react && npm test"
  },
  "author": {
    "name": "Jason Ellison",
    "email": "code@nosilleg.com"
  }
}
